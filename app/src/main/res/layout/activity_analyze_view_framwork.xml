<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/activity_analyze_view_framwork"
  android:layout_width="match_parent"
  android:layout_height="500dp"
  android:paddingBottom="@dimen/activity_vertical_margin"
  android:paddingLeft="@dimen/activity_horizontal_margin"
  android:paddingRight="@dimen/activity_horizontal_margin"
  android:paddingTop="@dimen/activity_vertical_margin"
  android:orientation="vertical"
  android:background="@color/blue"
  tools:context="com.chenbing.coorchicelibone.Views.AnalyzeViewFrameworkActivity">

  <com.chenbing.coorchicelibone.CustemViews.TouchEventTextView
    android:id="@+id/tv"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:gravity="center"
    android:text="Touch Me!"
    android:textColor="@color/black"
    android:textSize="30sp"
    android:textStyle="bold"
    android:enabled="true"
    />

  <TextView
      android:id="@+id/tv_1"
      android:layout_width="match_parent"
      android:layout_height="50dp"
      android:background="@color/a233be"
      android:visibility="gone"
      android:gravity="center"
      tools:visibility="visible"
      android:text="test"
      />

</LinearLayout>
